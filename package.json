{
    "name": "humanizrr",
    "version": "3.0.0",
    "description": "humanizrr provides methods for humanizing strings for AI-generated content.",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start": "node dist/playground.js",
        "dev": "ts-node-dev --respawn --transpile-only src/playground.ts",
        "test": "jest",
        "lint": "eslint src --ext .ts",
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LalbaAnthony/humanizrr.git"
    },
    "keywords": [
        "humanizrr",
        "humanize",
        "typos",
        "grammar",
        "spelling",
        "misspellings",
        "AI",
        "data",
        "content"
    ],
    "author": "Anthony Lalba",
    "license": "MIT",
    "dependencies": {
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^24.2.1",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "eslint": "^9.33.0",
        "eslint-plugin-node": "^11.1.0",
        "globals": "^16.3.0",
        "jest": "^30.2.0",
        "jiti": "^2.5.1",
        "nodemon": "^3.1.10",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.39.0"
    }
}